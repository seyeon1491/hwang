<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
fx:controller="page.ExamController">
   <top>
      <Label fx:id="title" text="Label" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button fx:id="next" mnemonicParsing="false" text="다음" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="qq0" text="Label" />
            <TextField fx:id="answer0" />
            <Label fx:id="qq1" text="Label" />
            <TextField fx:id="answer1" />
            <Label fx:id="qq2" text="Label" />
            <TextField fx:id="answer2" />
            <Label fx:id="qq3" text="Label" />
            <TextField fx:id="answer3" />
         </children>
      </VBox>
   </center>
</BorderPane>
